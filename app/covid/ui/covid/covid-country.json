                "getOption": "\nfunction parse_grafana_data(d) {\n  var rawData = []\n  if (data.series[0].fields[0].values.buffer === undefined) {\n    rawData = d.series[0].fields.map((e) => {\n      return e.values\n    })\n  } else { // large dataset will have their data inside a buffer\n    rawData = d.series[0].fields.map((e) => {\n      return e.values.buffer\n    })\n  }\n  var nDim = rawData.length\n  var nItem = rawData[0].length\n  var result = []\n  for (var i = 0; i < nItem; i++) {    // item\n    result[i] = []\n    for (var j = 0; j < nDim; j++) {  // dimension\n      result[i][j] = rawData[j][i]\n    }\n  }\n  return result\n}\n\nvar rawData = parse_grafana_data(data)\nvar dateList = rawData.map((i) => { return { name: i[0], value: i[1] } })\n\n// event: jump to station main page when click symbol\nechartsInstance.on(\"click\", function (params) {\n  console.log(params)\n  window.location = '/d/covid-country?var-country=' + params.data[\"name\"]\n})\n\nreturn {\n  title: {\n    text: 'COVID-19 Death Ratio \u2030 (Death per 1000 cases)',\n    subtext: 'Data Source: WHO',\n    top: '2%',\n    right: 'center'\n  },\n  grid: {\n    left: \"10%\",\n    bottom: \"10%\",\n  },\n  geo: {\n    show: false,\n    map: 'world',\n    zoom: 1.5\n  },\n  visualMap: {\n    show: true,\n    right: '5%',\n    top: '40%',\n    min: 0,\n    max: 100,\n    inRange: {\n      color: ['#ffffff', '#ffffbf', '#fee090', '#fdae61', '#f46d43', '#d73027', '#a50026']\n    },\n    text: ['High', 'Low'],           // \u6587\u672c\uff0c\u9ed8\u8ba4\u4e3a\u6570\u503c\u6587\u672c\n    calculable: true\n  },\n  series: [\n    {\n      name: \"covid\",\n      type: 'map',\n      map: 'world',\n      roam: false,\n      coordinateSystem: 'geo',\n      data: dateList,\n    }\n  ]\n}",
                "renderer": "canvas"
            },
