{
  "service": {
    "name": "haproxy",
    "port": 9101,
    "tags": [
      "{{ cluster }}",
      "{{ lb }}",
      "exporter"
    ],
    "meta": {
      "type": "exporter",
      "role": "{{ lb }}",
      "seq": "{{ seq }}",
      "instance": "{{ cluster }}-{{seq}}",
      "cluster": "{{ cluster }}"
    },
    "check": {
      "tcp": "localhost:9101",
      "interval": "5s",
      "timeout": "1s"
    }
  }
}