{
  "service": {
    "name": "{{ cluster }}",
    "port": 6432,
    "tags": [
      "{{ role }}",
      "{{ seq }}"
    ],
    "meta": {
      "type": "postgres",
      "role": "{{ role }}",
      "seq": "{{ seq }}",
      "instance": "{{ cluster }}-{{seq}}",
      "service": "{{ cluster }}-{{ role }}",
      "cluster": "{{ cluster }}",
      "version": "{{ version }}"
    },
    "check": {
      "http": "{{ inventory_hostname }}:8008/{{ role }}",
      "interval": "5s",
      "timeout": "1s"
    }
  }
}