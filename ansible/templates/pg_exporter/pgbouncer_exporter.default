PG_EXPORTER_URL='postgres://{{ mon_user }}:{{ mon_pass }}@localhost:6432/pgbouncer?host=/var/run/postgresql&sslmode=disable'
PG_EXPORTER_CONFIG=/etc/pg_exporter
PG_EXPORTER_LISTEN_ADDRESS=":9631"
PG_EXPORTER_TELEMETRY_PATH="/metrics"
PG_EXPORTER_DISABLE_CACHE=false
PG_EXPORTER_OPTS='--log.level=info --log.format="logger:syslog?appname=pgbouncer_exporter&local=7"'